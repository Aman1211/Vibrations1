
<!DOCTYPE html>
<html lang="en">

<%- include("../style.ejs") %>
<body>
  <div class="container-scroller">
    <!-- partial:partials/_navbar.html -->
  
    <!-- partial -->

    <%- include("../head1.ejs") %>
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar.html -->
     
      <!-- partial -->
     
      <%- include("../sidebar") %>
     
      <div class="main-panel">
        <div class="content-wrapper">


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Members Details</h4>
        
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th>
                  Name
                </th>
                <th>
                  Email
                </th>
                <th>
                  Contact
                </th>
                <th>
                  Stream
                </th>
                <th>
                  Year
                </th>
                <th>
                  Remove
                </th>
              </tr>
            </thead>
            <tbody>
                <% for(let i=0;i < info.length; i++) { %>
                   
                   
              <tr>
                <td class="py-1">
                  <%= info[i].name %>
                </td>
                <td>
                  <%= info[i].email %>
                </td>
                <td>
                  <%= info[i].contact %>
                </td>
                <td>
                  <%= info[i].stream %>
                </td>
                <td>
                  <%= info[i].year %>
                </td>
                <td>
                  <a class="btn btn-info" href="/removeprocess_Member?stud=<%=info[i]._id%>">Remove</a>
                </td>
              </tr>
              <%   }%>
            </tbody>
          </table>
        </div>
        <div style="padding-top:5px;margin-left: 3px;">
          <% let k=1 %>
          <%  for(let i=1 ; i <= count; i=i+5) { %>
             
            <a class="btn btn-info" href="/iview_Member?page=<%= k %>"><%= k %></a>
            <% k++ %>
            <% } %>
        </div>
      </div>
    </div>
  </div>
  <%- include("../footer") %>
  </div>
  </div>
</div>
</div>
</body>
</html>